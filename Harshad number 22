import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int number = a;
        int sum = 0;
        while(a >0) {
            sum += a % 10;
           a /= 10;
        }
        if (number % sum==0){
            System.out.println("Harshad Number");
        } else {
            System.out.println("Not Harshad Number");
        }    
    }
}
