import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine(); 
        StringBuffer obj = new StringBuffer(a);
        System.out.println(obj.reverse());   
    }
}
